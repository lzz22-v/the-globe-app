{
  "expo": {
    "name": "theGlobe",
    "slug": "rpg-casting-app",
    "version": "2.2.2",
    "orientation": "portrait",
    "icon": "./assets/globe-logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/globe-splash-logo.png",
      "resizeMode": "cover", 
      "backgroundColor": "#0b0b0b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lzz22.theglobe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/globe-logo.png",
        "backgroundColor": "#0b0b0b"
      },
      "package": "com.lzz22.theglobe",
      "permissions": [],
      "splash": {
        "image": "./assets/globe-splash-logo.png",
        "resizeMode": "cover",
        "backgroundColor": "#0b0b0b"
      }
    },
    "web": {
      "favicon": "./assets/globe-logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0b0b0b",
          "image": "./assets/globe-splash-logo.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f65bda1-7e10-4ea7-b8de-699615ef4165"
      }
    }
  }
}